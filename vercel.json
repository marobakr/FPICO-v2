{
  "version": 2,
  "buildCommand": "npm run build:ssr",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "npm run build:ssr",
        "outputDirectory": "dist/fpico-latest"
      }
    },
    {
      "src": "dist/fpico-latest/browser/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/dist/fpico-latest/browser/assets/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*\\.(?:js|css|webp|jpg|jpeg|png|svg|ico)$)",
      "dest": "/dist/fpico-latest/browser/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/dist/fpico-latest/server/server.mjs"
    }
  ]
}